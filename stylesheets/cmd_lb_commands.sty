% ---------------------- Individual Deck Statistics Calculator - Specific 1v1 --------------------- %
\newcommand{\calculateTurnNumberStatistics}[2]{%
	\FPset{deckMaxTurn}{0}%
	\FPset{deckMinTurn}{10}%
	\FPset{deckUpperQuartileTurn}{0}%
	\FPset{deckLowerQuartileTurn}{0}%
	\FPset{deckMedianTurn}{0}%
	\FPset{deckAverageTurn}{0}%
	
	\FPset{turnTotal}{0}%
	
	\setcounter{entrynumber}{0}%
	\DTLforeach{#2}{\deckname=DeckName, \turnnumber=TurnNumber}{%
		\ifthenelse{\equal{\deckname}{#1}}{%
			\stepcounter{entrynumber}
			\FPeval{turnTotal}{clip(turnTotal + \turnnumber)}
		}{}%
	}%
	
	\FPset{previousTurnCount}{0}%
	\FPset{tempCount}{0}%
	
	\ifthenelse{\equal{\theentrynumber}{0}}{}{%
		\FPeval{deckAverageTurn}{round(turnTotal/\theentrynumber, 2)}
		
		\calculateBoxPlotIndices{\theentrynumber}%
	
		\DTLsort{TurnNumber}{#2}%
		
		\DTLforeach{#2}{\deckname=DeckName, \turnnumber=TurnNumber}{%
			\ifthenelse{\equal{\deckname}{#1}}{%
				\FPeval{tempCount}{clip(tempCount + 1)}
				
				\ifnum\deckMaxTurn<\turnnumber%
					\FPset{deckMaxTurn}{\turnnumber}%
				\fi
				\ifnum\deckMinTurn>\turnnumber%
					\FPset{deckMinTurn}{\turnnumber}%
				\fi
				
				% Calculate Lower Quartile
				\ifdim\tempCount pt<\deckLowerQuartileIndex pt\else
					\ifdim\deckLowerQuartileTurn pt=0pt%
						\FPeval{isInteger}{clip(\deckLowerQuartileIndex - round(\deckLowerQuartileIndex,0))}
						\ifdim\isInteger pt = 0pt
							\FPset{\deckLowerQuartileTurn}{\turnnumber}
						\else
							\FPeval{\deckLowerQuartileTurn}{round((\turnnumber + \previousTurnCount)/ 2, 2)}
						\fi
					\fi%
				\fi%
				
				% Calculate Upper Quartile
				\ifdim\tempCount pt<\deckUpperQuartileIndex pt\else
					\ifdim\deckUpperQuartileTurn pt=0pt%
						\FPeval{isInteger}{clip(\deckUpperQuartileIndex - round(\deckUpperQuartileIndex,0))}
						\ifdim\isInteger pt = 0pt
							\FPset{\deckUpperQuartileTurn}{\turnnumber}
						\else
							\FPeval{\deckUpperQuartileTurn}{round((\turnnumber + \previousTurnCount)/ 2, 2)}
						\fi
					\fi%
				\fi%
				
				% Calculate Median
				\ifdim\tempCount pt<\deckMedianIndex pt\else
					\ifdim\deckMedianTurn pt=0pt%
						\FPeval{isInteger}{clip(\deckMedianIndex - round(\deckMedianIndex,0))}
						\ifdim\isInteger pt = 0pt
							\FPset{\deckMedianTurn}{\turnnumber}
						\else
							\FPeval{\deckMedianTurn}{round((\turnnumber + \previousTurnCount)/ 2, 2)}
						\fi
					\fi%
				\fi%
				
				\FPset{previousTurnCount}{\turnnumber}%
			}{}%
		}%
	}%
	
	\xdef\deckMaximumTurn{\deckMaxTurn}\relax
	\xdef\deckMinimumTurn{\deckMinTurn}\relax
	\xdef\deckLowerQuartileTurn{\deckLowerQuartileTurn}\relax
	\xdef\deckUpperQuartileTurn{\deckUpperQuartileTurn}\relax
	\xdef\deckMedianTurn{\deckMedianTurn}\relax
	\xdef\deckAverageTurn{\deckAverageTurn}\relax
}%
\newcommand{\calculateBoxPlotIndices}[1]{%
	\FPset{medianindex}{0}
	\FPset{lowerquartileindex}{0}
	\FPset{upperquartileindex}{0}

	\ifnum#1=1
		\FPset{medianindex}{1}
		\FPset{lowerquartileindex}{1}
		\FPset{upperquartileindex}{1}
	\fi
	
	\ifnum#1>1
		\FPeval{medianindex}{clip((#1+1)/2)}
		\FPeval{isInteger}{clip(\medianindex - round(\medianindex,0))}
		\ifdim\isInteger pt = 0pt
			\FPeval{npoints}{\medianindex-1}
		\else
			\FPeval{lowerindex}{round(\medianindex -1, 0)}
			\FPset{npoints}{\lowerindex}
		\fi
		
		% Calculate Lower and Upper Quartile Indices
		\FPeval{lowerquartileindex}{clip((\npoints+1)/2)}
		\FPeval{upperquartileindex}{clip(#1 - \lowerquartileindex + 1)}
		
		\xdef\medianindex{\medianindex}\relax
		\xdef\lowerquartileindex{\lowerquartileindex}\relax
		\xdef\upperquartileindex{\upperquartileindex}\relax
	\fi
	
	\xdef\deckMedianIndex{\medianindex}\relax
	\xdef\deckLowerQuartileIndex{\lowerquartileindex}\relax
	\xdef\deckUpperQuartileIndex{\upperquartileindex}\relax
}%
