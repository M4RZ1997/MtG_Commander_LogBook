\newcommand{\calculateFourPlayerStartGameWinLosePercentage}[1]{
	\setcounter{entrynumber}{\DTLrowcount{#1}}
	
	\setcounter{fourplayerfirstwinnumber}{0}
	\setcounter{fourplayersecondwinnumber}{0}
	\setcounter{fourplayerthirdwinnumber}{0}
	\setcounter{fourplayerfourthwinnumber}{0}
	\DTLforeach{#1}{\firstplayer=FirstPlayerFinish, \secondplayer=SecondPlayerFinish, \thirdplayer=ThirdPlayerFinish, \fourthplayer=FourthPlayerFinish}{%
		\ifnum\firstplayer=1%
			\stepcounter{fourplayerfirstwinnumber}%
		\fi%
		\ifnum\secondplayer=1%
			\stepcounter{fourplayersecondwinnumber}%
		\fi%
		\ifnum\thirdplayer=1%
			\stepcounter{fourplayerthirdwinnumber}%
		\fi%
		\ifnum\fourthplayer=1%
			\stepcounter{fourplayerfourthwinnumber}%
		\fi%
	}%
	
	\FPset{fourplayerfirstwinpercentage}{0}
	\FPeval{fourplayerfirstwinpercentage}{round(\thefourplayerfirstwinnumber/\theentrynumber*100,2)}
	\xdef\fourplayerfirstwinpercentage{\fourplayerfirstwinpercentage}\relax
	\FPset{fourplayersecondwinpercentage}{0}
	\FPeval{fourplayersecondwinpercentage}{round(\thefourplayersecondwinnumber/\theentrynumber*100,2)}
	\xdef\fourplayersecondwinpercentage{\fourplayersecondwinpercentage}\relax
	\FPset{fourplayerthirdwinpercentage}{0}
	\FPeval{fourplayerthirdwinpercentage}{round(\thefourplayerthirdwinnumber/\theentrynumber*100,2)}
	\xdef\fourplayerthirdwinpercentage{\fourplayerthirdwinpercentage}\relax
	\FPset{fourplayerfourthwinpercentage}{0}
	\FPeval{fourplayerfourthwinpercentage}{round(\thefourplayerfourthwinnumber/\theentrynumber*100,2)}
	\xdef\fourplayerfourthwinpercentage{\fourplayerfourthwinpercentage}\relax
}
