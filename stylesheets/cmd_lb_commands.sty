\newcommand{\countOccurences}[2]{%
	\DTLforeach{#1}{\deckname=DeckName, \winorlose=WinOrLose, \wincondition=WinCondition, \turnnumber=TurnNumber, \colidentity=WinnerColorIdentity, \obviousproblem=ObviousProblems, \mulligannumber=MulliganNumber, \mulligancauses=MulliganCause}{%
		\ifthenelse{\equal{\deckname}{#2}}{%
			\stepcounter{entrynumber}%
			
			\ifthenelse{\equal{\winorlose}{WIN}}{%
				\stepcounter{deckwinnumber}%
				% HP0, Poison, DeckOut, CardEffect, CommanderDamage
				\ifthenelse{\equal{\wincondition}{HP0}}{\stepcounter{deckwinhponumber}}{}%
				\ifthenelse{\equal{\wincondition}{Poison}}{\stepcounter{deckwinpoisonnumber}}{}%
				\ifthenelse{\equal{\wincondition}{DeckOut}}{\stepcounter{deckwindeckoutnumber}}{}%
				\ifthenelse{\equal{\wincondition}{CardEffect}}{\stepcounter{deckwincardeffectnumber}}{}%
				\ifthenelse{\equal{\wincondition}{CommanderDamage}}{\stepcounter{deckwincommanderdamagenumber}}{}%
			}{%
				\stepcounter{decklosenumber}%
				% HP0, Poison, DeckOut, CardEffect, CommanderDamage
				\ifthenelse{\equal{\wincondition}{HP0}}{\stepcounter{decklosehponumber}}{}%
				\ifthenelse{\equal{\wincondition}{Poison}}{\stepcounter{decklosepoisonnumber}}{}%
				\ifthenelse{\equal{\wincondition}{DeckOut}}{\stepcounter{decklosedeckoutnumber}}{}%
				\ifthenelse{\equal{\wincondition}{CardEffect}}{\stepcounter{decklosecardeffectnumber}}{}%
				\ifthenelse{\equal{\wincondition}{CommanderDamage}}{\stepcounter{decklosecommanderdamagenumber}}{}%
				
				\checkCharacter{\colidentity}
			}%
			
			\ifthenelse{\turnnumber<\shortgamelength}{%
				\stepcounter{deckshortgamenumber}%
			}{%
				\ifthenelse{\turnnumber<\midgamelength}{%
					\stepcounter{deckmidgamenumber}
				}{%
					\ifthenelse{\turnnumber<\longgamelength}{%
						\stepcounter{decklonggamenumber}%
					}{%
						\stepcounter{deckverylonggamenumber}%
					}%
				}%
			}%
			
			\ifthenelse{\equal{\obviousproblem}{ManaScrew}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckopmanascrewnumber}}{}%
			\ifthenelse{\equal{\obviousproblem}{ManaFlood}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckopmanafloodnumber}}{}%
			\ifthenelse{\equal{\obviousproblem}{CardDraw}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckopcarddrawnumber}}{}%
			\ifthenelse{\equal{\obviousproblem}{LackOfResponses}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckoplackofresponsesnumber}}{}%
			\ifthenelse{\equal{\obviousproblem}{HighCMC}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckophighcmcnumber}}{}%
			\ifthenelse{\equal{\obviousproblem}{Targeting}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckoptargetingnumber}}{}%
			\ifthenelse{\equal{\obviousproblem}{Misplays}}{\stepcounter{deckobviousproblemsnumber}\stepcounter{deckopmisplaysnumber}}{}%
			
			\ifthenelse{\mulligannumber>0}{%
				\FPeval{deckTotalMulliganNumber}{clip(deckTotalMulliganNumber + \mulligannumber)}%
				
				\IfSubStr{\mulligancauses}{ManaScrew}{\stepcounter{deckmcmanascrewnumber}}{}%
				\IfSubStr{\mulligancauses}{ManaFlood}{\stepcounter{deckmcmanafloodnumber}}{}%
				\IfSubStr{\mulligancauses}{BadManaCurve}{\stepcounter{deckmcbadmanacurvenumber}}{}%
				\IfSubStr{\mulligancauses}{ActionLack}{\stepcounter{deckmcactionlacknumber}}{}%
				\IfSubStr{\mulligancauses}{DeckSpecific}{\stepcounter{deckmcdeckspecificnumber}}{}%
			}{}%
		}{}%
	}%
}%
\newcommand{\checkCharacter}[1]{%
	\IfSubStr{#1}{C}{\stepcounter{winnercolorlessnumber}}{}%
	\IfSubStr{#1}{W}{\stepcounter{winnerwhitenumber}}{}%
	\IfSubStr{#1}{U}{\stepcounter{winnerbluenumber}}{}%
	\IfSubStr{#1}{B}{\stepcounter{winnerblacknumber}}{}%
	\IfSubStr{#1}{R}{\stepcounter{winnerrednumber}}{}%
	\IfSubStr{#1}{G}{\stepcounter{winnergreennumber}}{}%
}%
