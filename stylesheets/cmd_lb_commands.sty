\newcommand{\calculateTwoPlayerSpecificDeckStatistics}[2]{%
	\setcounter{entrynumber}{0}%
	
	\setcounter{deckTwoPlayerFastManaWins}{0}%
	\setcounter{deckTwoPlayerFastManaLoses}{0}%
	\setcounter{deckTwoPlayerNoFastManaWins}{0}%
	\setcounter{deckTwoPlayerNoFastManaLoses}{0}%
	
	\setcounter{deckTwoPlayerFirstPlayerWins}{0}%
	\setcounter{deckTwoPlayerSecondPlayerWins}{0}%
	
	\DTLforeach{#2}{\deckname=DeckName, \winorlose=WinOrLose, \fastmana=FastMana}{%
		\ifthenelse{\equal{\deckname}{#1}}{%
			\stepcounter{entrynumber}
			\ifthenelse{\equal{\winorlose}{WIN}}{%
				\stepcounter{deckTwoPlayerFirstPlayerWins}%
				\ifthenelse{\equal{\fastmana}{SELF}}{%
					\stepcounter{deckTwoPlayerFastManaWins}%
					\stepcounter{deckTwoPlayerNoFastManaLoses}%
				}{}%
				\ifthenelse{\equal{\fastmana}{OPP}}{%
					\stepcounter{deckTwoPlayerNoFastManaWins}%
					\stepcounter{deckTwoPlayerFastManaLoses}%
				}{}%
				\ifthenelse{\equal{\fastmana}{BOTH}}{%
					\stepcounter{deckTwoPlayerFastManaWins}%
					\stepcounter{deckTwoPlayerFastManaLoses}%
				}{}%
				\ifthenelse{\equal{\fastmana}{NONE}}{%
					\stepcounter{deckTwoPlayerNoFastManaWins}%
					\stepcounter{deckTwoPlayerNoFastManaLoses}%
