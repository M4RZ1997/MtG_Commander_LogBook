% ------------------------------------ Most/Least  Played Deck ------------------------------------ %
\newcommand{\calcMostLeastTrackedDeck}[1]{%
	\DTLforeach{CommanderDecks}{\tempDeckName=DeckName, \tempImage=Image}{%
		\setcounter{tempTrackNumber}{0}
		\DTLforeach{LogBook1v1}{\lbDeckName=DeckName}{%
			\ifthenelse{\equal{\tempDeckName}{\lbDeckName}}{\stepcounter{tempTrackNumber}}{}%
		}%
		\DTLforeach{LogBook3Player}{\lbDeckName=DeckName}{%
			\ifthenelse{\equal{\tempDeckName}{\lbDeckName}}{\stepcounter{tempTrackNumber}}{}%
		}%
		\DTLforeach{LogBook4Player}{\lbDeckName=DeckName}{%
			\ifthenelse{\equal{\tempDeckName}{\lbDeckName}}{\stepcounter{tempTrackNumber}}{}%
		}%
		\ifthenelse{\equal{#1}{>} \AND \equal{\mlTimesTrackedNumber}{-1}}{
			\xdef\mlTimesTrackedNumber{\thetempTrackNumber}\relax%
			\xdef\mlTrackedDeck{\tempDeckName}\relax%
			\xdef\DeckImage{\tempImage}\relax%
		}{}
		\ifthenelse{\mlTimesTrackedNumber #1 \thetempTrackNumber}{%
			\xdef\mlTimesTrackedNumber{\thetempTrackNumber}\relax%
			\xdef\mlTrackedDeck{\tempDeckName}\relax%
			\xdef\DeckImage{\tempImage}\relax%
		}{}%
	}%
}%
