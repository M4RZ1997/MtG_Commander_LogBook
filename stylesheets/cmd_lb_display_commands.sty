% --------------------------- First Page:  Latest Performance Statistic --------------------------- %		
\newcommand{\fixedchar}[1]{\makebox[1em]{#1}} % Adjust the width (1em) as needed
		
\newcommand{\calcPerformanceStatistic}[1]{%
	\setcounter{TotalEntries}{\DTLrowcount{#1}}%
	\setcounter{CurrentEntry}{0}
	\hspace*{0.5em}Number of Games:~\theTotalEntries\\\hspace*{0.5em}Last 10 Games:\vspace*{-0.75em}%
	\begin{center}%
		\DTLforeach{#1}{\WinOrLose=WinOrLose}{%
			\stepcounter{CurrentEntry}%
			\ifnum\value{CurrentEntry}>\numexpr\value{TotalEntries}-10\relax%
				\ifthenelse{\equal{\WinOrLose}{WIN}}{%
					\fixedchar{\textcolor{MarzGreen}{\textbf{W}}}%
				}{%
					\fixedchar{\textcolor{MarzDarkRed}{\textbf{L}}}%
				}%
			\fi%					
		}%
		\ifthenelse{\value{TotalEntries}<10}{%
			\setcounter{tempEntryCounter}{\numexpr\value{TotalEntries}-10\relax}%
			\whiledo{\value{tempEntryCounter} < 0}{%
      			\fixedchar{~}%
      			\stepcounter{tempEntryCounter}%
    		}%
    		}{}%
	\end{center}%
}%
