% ----------------------------------- PreCon WIN/LOSE Statistic ----------------------------------- %
\newcounter{tempPreConWinNumber}
\newcommand{\framePreConWinLoseStatistic}{%
	\setcounter{tempPreConWinNumber}{0}%
	\pgfmathsetmacro{\precongamestotal}{\DTLrowcount{LogBookPreCon1v1} + \DTLrowcount{LogBookPreCon3Player} + \DTLrowcount{LogBookPreCon4Player}}%
	\DTLforeach{LogBookPreCon1v1}{\lbWinOrLose=WinOrLose}{%
		\ifthenelse{\equal{WIN}{\lbWinOrLose}}{\stepcounter{tempPreConWinNumber}}{}%
	}%
	\DTLforeach{LogBookPreCon3Player}{\lbWinOrLose=WinOrLose}{%
		\ifthenelse{\equal{WIN}{\lbWinOrLose}}{\stepcounter{tempPreConWinNumber}}{}%
	}%
	\DTLforeach{LogBookPreCon4Player}{\lbWinOrLose=WinOrLose}{%
		\ifthenelse{\equal{WIN}{\lbWinOrLose}}{\stepcounter{tempPreConWinNumber}}{}%
	}%
	\FPset{preconwinpercentage}{0}%
	\FPeval{preconwinpercentage}{round(\thetempPreConWinNumber/\precongamestotal*100,2)}%
	\FPeval{preconlosepercentage}{round(100-\preconwinpercentage,2)}%
	\frameWinLoseStatistic{\preconwinpercentage}{\preconlosepercentage}%
}%
