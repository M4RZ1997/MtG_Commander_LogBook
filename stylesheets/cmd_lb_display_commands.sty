% Calculation
\newcommand{\calcMostLeastPreConColorIdentity}[1]{%
	\foreach \lColorIdentity in \listColorCombinations {%
		\setcounter{tempAppearanceNumber}{0}
		\DTLforeach{LogBookPreCon1v1}{\lbDeckColor=DeckColor}{%
			\ifthenelse{\equal{\lColorIdentity}{\lbDeckColor}}{\stepcounter{tempAppearanceNumber}}{}%
		}%
		\DTLforeach{LogBookPreCon3Player}{\lbDeckColor=DeckColor}{%
			\ifthenelse{\equal{\lColorIdentity}{\lbDeckColor}}{\stepcounter{tempAppearanceNumber}}{}%
		}%
		\DTLforeach{LogBookPreCon4Player}{\lbDeckColor=DeckColor}{%
			\ifthenelse{\equal{\lColorIdentity}{\lbDeckColor}}{\stepcounter{tempAppearanceNumber}}{}%
		}%
		\ifthenelse{\equal{#1}{>} \AND \equal{\mlTimesAppearanceNumber}{-1}}{
			\xdef\mlTimesAppearanceNumber{\thetempAppearanceNumber}\relax%
			\xdef\mlColorIdentity{\lColorIdentity}\relax%
			\xdef\mlColorIdentityName{\getKeyValue{\lColorIdentity}}\relax%
		}{}
		\ifthenelse{\mlTimesAppearanceNumber #1 \thetempAppearanceNumber}{%
			\xdef\mlTimesAppearanceNumber{\thetempAppearanceNumber}\relax%
			\xdef\mlColorIdentity{\lColorIdentity}\relax%
			\xdef\mlColorIdentityName{\getKeyValue{\lColorIdentity}}\relax%
		}{}%
	}%
}%
