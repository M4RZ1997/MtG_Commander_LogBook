% Char-in-String Check
\makeatletter
\def\instring#1#2{TT\fi\begingroup
  \edef\x{\endgroup\noexpand\in@{#1}{#2}}\x\ifin@}
\makeatother
% Calculation and Plotter
\newcounter{tempColorCounter}%
\newcounter{tempColorWinCounter}%
\newcounter{tempHelper}%
\newcommand{\printPreConColorWinDistribution}{%
	\begin{tikzpicture}
		\node at (0,0) {};
		
		% Color Icons
		\filldraw[fill=white] (1cm,-0.4cm) circle (5pt);%
		\node[inner sep=0pt] at (1.025cm,-0.2cm) {\textcolor{black}{\LARGE \MtGIcons{x}}};%
		\filldraw[fill=white] (1.5cm,-0.4cm) circle (5pt);%
		\node[inner sep=0pt] at (1.525cm,-0.2cm) {\textcolor{black}{\LARGE \MtGIcons{a}}};%
		\filldraw[fill=white] (2cm,-0.4cm) circle (5pt);%
		\node[inner sep=0pt] at (2.025cm,-0.2cm) {\textcolor{MarzRoyal!80}{\LARGE \MtGIcons{b}}};%
		\filldraw[fill=white] (2.5cm,-0.4cm) circle (5pt);%
		\node[inner sep=0pt] at (2.525cm,-0.2cm) {\textcolor{MarzPurple}{\LARGE \MtGIcons{c}}};%
		\filldraw[fill=white] (3cm,-0.4cm) circle (5pt);%
		\node[inner sep=0pt] at (3.025cm,-0.2cm) {\textcolor{MarzDarkRed}{\LARGE \MtGIcons{d}}};%
		\filldraw[fill=white] (3.5cm,-0.4cm) circle (5pt);%
		\node[inner sep=0pt] at (3.525cm,-0.2cm) {\textcolor{MarzGreen!80}{\LARGE \MtGIcons{e}}};%
		
		\foreach \y in {0,...,4} {%
			\node[anchor=east] at (0.5cm,\fpeval{-1.25*\y - 1}cm) {\fpeval{25*\y}\%};%
		}%
		\draw[gray, very thick, text=white] (0.6cm,-0.2cm) -- (0.6cm,-6.25cm);%
		
		\setcounter{tempHelper}{0}%
		\foreach \color in {C, W, U, B, R, G} {%
			\stepcounter{tempHelper}
			\setcounter{tempColorCounter}{0}%
			\setcounter{tempColorWinCounter}{0}%
			\DTLforeach{LogBookPreCon1v1}{\tempDeckColor=DeckColor, \tempWinOrLose=WinOrLose}{%
				\if\instring{\color}{\tempDeckColor}%
					\stepcounter{tempColorCounter}%
					\ifthenelse{\equal{WIN}{\tempWinOrLose}}{%
						\stepcounter{tempColorWinCounter}%
					}{}%
				\else\fi%
			}%
			\DTLforeach{LogBookPreCon3Player}{\tempDeckColor=DeckColor, \tempWinOrLose=WinOrLose}{%
				\if\instring{\color}{\tempDeckColor}%
					\stepcounter{tempColorCounter}%
					\ifthenelse{\equal{WIN}{\tempWinOrLose}}{%
						\stepcounter{tempColorWinCounter}%
					}{}%
				\else\fi%
			}%
			\DTLforeach{LogBookPreCon4Player}{\tempDeckColor=DeckColor, \tempWinOrLose=WinOrLose}{%
				\if\instring{\color}{\tempDeckColor}%
					\stepcounter{tempColorCounter}%
					\ifthenelse{\equal{WIN}{\tempWinOrLose}}{%
						\stepcounter{tempColorWinCounter}%
					}{}%
				\else\fi%
