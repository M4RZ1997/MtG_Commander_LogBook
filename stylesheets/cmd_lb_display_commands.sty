				\node[xshift=3.5cm,yshift=0.7cm] (Header) {\textcolor{white}{\Huge{\titlefont{Color Combination Distribution}}}};%
				\node at (20cm, 0cm) {};
				\pgfmathsetmacro{\maxValue}{\mlTimesAppearanceNumber}%
				\pgfmathsetmacro{\sum}{\DTLrowcount{LogBookPreCon1v1} + \DTLrowcount{LogBookPreCon3Player} + \DTLrowcount{LogBookPreCon4Player}}%
				
%				\draw[gray, very thick] (0cm,0cm) -- (20cm,0cm);%
%				\foreach \x in {1,...,3}%
%    				\draw[gray!50] (\fpeval{3*\x + 0.5} cm,0.15cm) -- (\fpeval{3*\x + 0.5} cm,0cm);%
				
				%\pgfmathsetmacro{\maxValue}{1}
				\pgfmathsetmacro{\maxValue}{\maxValue == 0 ? 1 : \maxValue}
				%\pgfmathsetmacro{\sum}{1}
				\pgfmathsetmacro{\sum}{\sum == 0 ? 1 : \sum}
				
				\draw[gray, very thick, text=white] (-0.5cm,0.3cm) -- (-0.5,-5cm);%
				
				\foreach \y in {1,...,3}%
					\draw[gray!50] (-0.65cm,\fpeval{1.1*\y -4.4}cm) -- (-0.5cm,\fpeval{1.1*\y -4.4}cm);%
				\draw[gray, very thick, text=white] (-0.7cm,0cm) -- (-0.3cm,0cm) node at (-1.25cm,0cm) {\small{\fpeval{round(\maxValue/\sum*100,2)}\%}};%
				\draw[gray, very thick, text=white] (-0.7cm,-4.4cm) -- (-0.3cm,-4.4cm) node at (-1.25cm,-4.4cm) {\small{0\%}};%
				
				\newcounter{tempStepCICounter}
				\setcounter{tempStepCICounter}{0}				
				\foreach \lColorIdentity in \listColorCombinations {%
					\stepcounter{tempStepCICounter}%
					
					\setcounter{tempAppearanceNumber}{0}
					\DTLforeach{LogBookPreCon1v1}{\lbDeckColor=DeckColor}{%
						\ifthenelse{\equal{\lColorIdentity}{\lbDeckColor}}{\stepcounter{tempAppearanceNumber}}{}%
					}%
					\DTLforeach{LogBookPreCon3Player}{\lbDeckColor=DeckColor}{%
